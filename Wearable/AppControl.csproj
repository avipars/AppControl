<Project Sdk="Tizen.NET.Sdk/1.0.9">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>tizen50</TargetFramework>
    <SignAssembly>False</SignAssembly>
    <StartupObject>AppControl.Program</StartupObject>
    <PackageId>AppControl</PackageId>
    <Authors>Avi Parshan</Authors>
    <Company>AmCan Tech</Company>
    <AssemblyName>AppControl</AssemblyName>
    <AssemblyOriginatorKeyFile>avi_key.pfx</AssemblyOriginatorKeyFile>
    <DelaySign>true</DelaySign>
    <Version>0.0.5</Version>
    <Title>Controller</Title>
    <Copyright>AmCan Tech</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>None</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="lib\" />
    <Folder Include="res\" />
  </ItemGroup>



  <ItemGroup>
    <PackageReference Include="Tizen.Wearable.CircularUI" Version="1.4.0" />
  </ItemGroup>

</Project>
